<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="net.practica.simplerssreader"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="19" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="net.practica.simplerssreader.activity.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="net.practica.simplerssreader.activity.AdminFeed" >
        </activity>
        <activity android:name="net.practica.simplerssreader.activity.DetailFeedActivity" >
        </activity>
        <activity android:name="net.practica.simplerssreader.activity.AdminFeedActivity" >
        </activity>
    </application>

</manifest>